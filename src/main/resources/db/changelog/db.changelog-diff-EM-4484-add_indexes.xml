            <not>
                <indexExists tableName="auditentry" indexName="auditentry_documentcontentversion_idx" columnNames="documentcontentversion_id"/>
            </not>
        </preConditions>
        <createIndex indexName="auditentry_documentcontentversion_idx" tableName="auditentry">
            <column name="documentcontentversion_id"/>
        </createIndex>
    </changeSet>

    <changeSet id="EM-4484-auditentry-2" author="yogesh (manual)">
        <preConditions onFail="MARK_RAN" onFailMessage="This index already exists and will not be created."
                       onError="MARK_RAN" onErrorMessage="This index already exists and will not be created." >
            <not>
                <indexExists tableName="auditentry" indexName="auditentry_storeddocument_idx" columnNames="storeddocument_id"/>
            </not>
        </preConditions>
        <createIndex indexName="auditentry_storeddocument_idx" tableName="auditentry">
            <column name="storeddocument_id"/>
        </createIndex>
    </changeSet>

</databaseChangeLog>
